<p id="notice"><%= notice %></p>

<h1>Trainingmenus</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trainingmenus.each do |trainingmenu| %>
      <tr>
        <td><%= trainingmenu.date %></td>
        <td><%= link_to 'Show', trainingmenu %></td>
        <td><%= link_to 'Edit', edit_trainingmenu_path(trainingmenu) %></td>
        <td><%= link_to 'Destroy', trainingmenu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trainingmenu', new_trainingmenu_path %>
